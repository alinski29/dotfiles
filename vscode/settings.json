{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Pylance",
    "kite.showWelcomeNotificationOnStartup": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "yaml.schemas": {
        "file:///toc.schema.json": "/toc\\.yml/i"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "go.toolsManagement.autoUpdate": true,
    "git.suggestSmartCommit": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "julia.execution.codeInREPL": true,
    "julia.execution.resultType": "inline",
    "julia.symbolCacheDownload": true,
    "workbench.colorTheme": "Atom One Dark",
    "editor.fontSize": 13,
    "editor.tabSize": 2,
    "editor.guides.indentation": false,
    "editor.fontFamily": "'FiraCode Nerd Font', 'JetBrains Mono Nerd Font', 'Cascadia Code',  'RobotoMono Nerd Font'",
    "julia.enableTelemetry": false,
    "julia.format.indent": 2,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.idea": true
    },
    "notebook.lineNumbers": "on",
    "zenMode.hideLineNumbers": false,
    "search.showLineNumbers": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        //"<A-l>": true,
        "<C-a>": true,
        "<C-g>": false
    },
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        },
        {
            "before": ["k", "j"],
            "after": ["<Esc>"]
        },
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        },
        {
            "before": ["k", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>", "h"],
            "commands": [
                ":nohls"
            ],
            "siltent": true
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#8FBCBB",
        "#000"
    ],
    "vim.statusBarColors.insert": [
        "#BF616A",
        "#000"
    ],
    "vim.statusBarColors.visual": [
        "#B48EAD",
        "#000"
    ],
    "vim.statusBarColors.visualline": [
        "#B48EAD",
        "#000"
    ],
    "vim.statusBarColors.visualblock": [
        "#A3BE8C",
        "#000"
    ],
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "workbench.colorCustomizations": {
        "statusBar.background": "#8FBCBB",
        "statusBar.noFolderBackground": "#8FBCBB",
        "statusBar.debuggingBackground": "#8FBCBB",
        "statusBar.foreground": "#000",
        "statusBar.debuggingForeground": "#000"
    },
    "[julia]": {
        "editor.defaultFormatter": "singularitti.vscode-julia-formatter"
    },
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorBlinking": "smooth",
    "workbench.iconTheme": "material-icon-theme",
    "julia.enableCrashReporter": false,
    "extensions.ignoreRecommendations": true,
    "vim.neovimUseConfigFile": true,
    "vim.neovimConfigPath": "~/config/nvim/init.lua",
    "juliaFormatter.alwaysForIn": true,
    "juliaFormatter.indent": 2,
    "juliaFormatter.removeExtraNewlines": true,
    "juliaFormatter.whitespaceOpsInIndices": true,
    "juliaFormatter.whitespaceTypedefs": true,
    "vim.enableNeovim": true,
    "vim.foldfix": true,
    "vim.leader": "\\<space>",
    "juliaFormatter.alignStructField": true,
    "juliaFormatter.style": "blue",
    "markdown.extension.tableFormatter.normalizeIndentation": true,
    "juliaFormatter.alwaysUseReturn": true,
    "juliaFormatter.formatDocstrings": true,
    "juliaFormatter.importToUsing": true,
    "markdown.extension.toc.updateOnSave": false
}
